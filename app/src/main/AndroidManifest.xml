<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pruebateatro">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PruebaTeatro"
        android:usesCleartextTraffic="true"
        android:name=".CapaDatos$SharedApp">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistrarUsuario" />
        <activity android:name=".PaginaPrincipal" />
        <activity android:name=".calendario" />
        <activity android:name=".compraTiquetes" />
        <activity android:name=".PreciosObra" />
    </application>

</manifest>